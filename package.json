    "@types/react-dom": "^19.2.2",
    "bootstrap": "^5.3.8",
    "clsx": "^2.1.1",
    "next": "15.5.5",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "tsx": "^4.20.6",
    "vercel": "^48.2.9"
